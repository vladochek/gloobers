    margin: 5px 0;
    cursor: pointer;
}
.pager li > a, .pager li > span { font-size: 14px }
.pager.pager-sm li > a, .pager.pager-sm li > span { font-size: 12px }
.pager.pager-lg li > a, .pager.pager-lg li > span { font-size: 16px }
