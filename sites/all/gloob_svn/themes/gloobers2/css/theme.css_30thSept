.pagination {
    margin: 5px 0;
    cursor: pointer;
}
/* ROUNDED PAGINATION */
.pagination-rounded > li:first-child > a { border-radius: 50% 0 0 50% }
.pagination-rounded > li:last-child > a { border-radius: 0 50% 50% 0 }
/* ALT STYLE */
.pagination-alt > li > a {
    margin: 0 4px;
    border-radius: 2px;
    font-weight: 600;
    color: #666;
    text-shadow: 0 1px #FFF;
    padding: 4px 11px;
