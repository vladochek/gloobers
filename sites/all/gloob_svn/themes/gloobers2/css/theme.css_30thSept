    min-height: 170px;
    padding: 16px 12px;
    border: 1px solid #CCC;
}
/* TAB NAVIGATION */
.nav-tabs {
    position: relative;
    top: 1px;
}
.nav-tabs > li {
    float: left;
    margin-bottom: -1px;
}
.nav-tabs > li > a {
    position: relative;
    z-index: 9;
    padding: 12px 12px;
    margin-right: -1px;
