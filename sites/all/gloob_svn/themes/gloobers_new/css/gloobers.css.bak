*{ margin:0; padding:0;} a{ outline:none;} img{ border:none;} .clear{ clear:both; }
body{font-size:12px; -webkit-font-smoothing:antialiased; font-family:"Open Sans", Arial, Helvetica, sans-serif;}
/****************************Homepage***********************************************/
.dr-wrapper{ max-width:1210px; margin:auto; width:100%;}
header{ float:left; width:100%; padding:18px 1%; position:fixed; transition:all ease 550ms;  z-index: 99;}
header .logo{ float:left;}
header .finder{ margin:auto; width:100%; max-width:190px; left:0; right:0; top:27px; position:absolute; }
header .finder a{ font-size:24px; color:#fff; text-decoration:none; }
header nav{ float:right; padding:10px 0 0 0;}
header nav ul{ list-style:none; }
header nav ul li{ float:left;}
header nav ul li a{ text-decoration:none; padding:0 20px; color:#fff; font-size:21px;}
header nav ul li a:hover{ color:#f5c400;}
.finder-link{ display:none;}
header.sticky{ transition:all ease 550ms; background:#212121; z-index:200;}
.bannersec{ float:left; width:100%; background:url(../images/bg1.jpg) center 0 no-repeat; background-size:100% 100%;}
.bannerstuf{ float:left; width:100%; text-align:center; padding:220px 0 195px;}
.bannerstuf h2{ color:#fff; font-size:40px; font-weight:700; text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5);}
.bannerstuf p{ color:#fff; font-size:39px; padding-bottom:55px;text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5); }
.bannerstuf .searchbar{ width:100%; height:75px; max-width:704px; margin:auto; border-radius:5px; padding:15px 0; background:rgba(0,0,0,0.4);}
.bannerstuf .searchbar .form-control { border-radius: 0;}
.bannerstuf .searchbar input[type=text]{ float:left; width:100%; background:#fff; height:45px; line-height:45px; padding:0 15px; color:#242424; border:none; font-size:19px; font-family:"Open Sans";}
.bannerstuf .searchbar input[type=submit]{ cursor: pointer;background:#f8bd00; float:right; height:45px; text-align:center; color:#212121; font-size:24px; font-weight:700; width:100%; border:none; text-transform:uppercase;}
.bgback1{ float:left; width:100%; padding:40px 0 40px 0; background:#f5f5f5;}
.workstuf{ float:left; width:100%; padding-bottom:90px;}
.workstuf h1{ text-align:center; font-size:36px; font-weight:700; padding-bottom:70px; color:#212121;}
.workstuf .leftsec{ float:left; }
.workstuf .leftsec img,.workstuf .rightsec2 img{ float:left; width:100%; border:5px solid #e2e2e4; box-shadow:6px 6px 7px #ccc;}
.workstuf .rightsec{ float:right; padding:10px 0 0 0; text-align:center;}
.workstuf .rightsec h2{ font-size:33px; font-weight:700; padding-bottom:20px; color:#212121; }
.workstuf .rightsec p{ font-size:30px; color:#212121;}
.workstuf .rightsec2{ float:right; width:100%; max-width:540px; position:relative;}
/* .workstuf .rightsec2 img{ float:left; width:100%;} */
/* .workstuf .rightsec2 .clientinfo{ position:absolute; bottom:-85px; left:40px; text-align:center;} */
.workstuf .rightsec2 .clientinfo {
  bottom: -86px;
  left: 0;
  position: absolute;
  text-align: center;
}
.workstuf .rightsec2 .clientinfo img{ margin-bottom:10px; border-radius:100%; width:70%; float:none;}
.workstuf .rightsec2 .clientinfo p{ color:#424242; font-size:18px; font-weight:bold;}
.workstuf .leftsec2{ float:left; width:100%; max-width:580px; padding:10px 0 0 0; text-align:center;}
.workstuf .leftsec2 h2{ font-size:33px; font-weight:700; color:#212121; padding-bottom:20px;}
.workstuf .leftsec2 p{ font-size:30px; color:#212121;}
.destistuf{ float:left; width:100%; padding:47px 0 75px 0;}
.destistuf h1{ text-align:center; font-size:36px; font-weight:700; padding-bottom:47px; color:#212121;}
.destistuf .block{ position:relative; margin-bottom:30px;}
.destistuf .block img{ float:left; width:100%;}
.destistuf .block .names{ position:absolute; top:40%; width:96%; text-align:center;}
.destistuf .block .names p{ color:#fff; text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5); font-size:28px; font-weight:700;}
.bgback2{float:left; width:100%; padding:138px 0 128px; background:url(../images/bg2.jpg) fixed center 0 no-repeat; background-size:100% 100%;}
.tourismsec{ float:left; width:100%; text-align:center;}
.tourismsec h1{ color:#fff; font-size:42px; font-weight:700; padding-bottom:15px;text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5);}
.tourismsec p{ color:#fff; font-size:39px; padding:0 80px 15px 80px;text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5); }
.tourismsec .btnlearn{ margin:auto; width:100%; max-width:212px;}
.tourismsec .btnlearn a,.recomm-sec .btn-recom a { width:100%; float:left; text-align:center; background:#f5c500; text-decoration:none; font-size:27px; color:#212121; height:44px; line-height:44px; font-weight:700;}
.socialsec{ float:left; width:100%; padding:90px 0; text-align:center;}
.socialsec h1{ text-align:center; font-size:36px; font-weight:700; padding-bottom:47px; color:#212121;}
footer{ float:left; width:100%; background: #232323; /* Old browsers */
background: -moz-linear-gradient(top, #232323 0%, #333333 50%, #414141 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#232323), color-stop(50%,#333333), color-stop(100%,#414141)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #232323 0%,#333333 50%,#414141 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #232323 0%,#333333 50%,#414141 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #232323 0%,#333333 50%,#414141 100%); /* IE10+ */
background: linear-gradient(to bottom, #232323 0%,#333333 50%,#414141 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#232323', endColorstr='#414141',GradientType=0 ); /* IE6-9 */ padding:30px 0;}
footer .firstcol{ float:left;}
footer .firstcol h2{ color:#fff; font-weight:700; font-size:18px; padding-bottom:10px;}
footer .firstcol ul{ list-style:none;}
footer .firstcol ul li{ float:left; width:100%; line-height:33px;}
footer .firstcol ul li a{ text-decoration:none; color:#fff; font-size:16px;}
footer .firstcol .btns{ float:left; width:100%; max-width:164px; margin-bottom:11px;}
footer .firstcol .btns a{ border:1px solid #acacac; float:left; width:100%; height:40px; line-height:40px; text-decoration:none; color:#fff; border-radius:5px; background: #404040; /* Old browsers */
background: -moz-linear-gradient(top, #404040 0%, #343434 50%, #252525 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#404040), color-stop(50%,#343434), color-stop(100%,#252525)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #404040 0%,#343434 50%,#252525 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #404040 0%,#343434 50%,#252525 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #404040 0%,#343434 50%,#252525 100%); /* IE10+ */
background: linear-gradient(to bottom, #404040 0%,#343434 50%,#252525 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#404040', endColorstr='#252525',GradientType=0 ); /* IE6-9 */ font-size:16px; font-weight:700; text-transform:uppercase; padding:0 14px; text-align:right; text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5);}
footer .firstcol .btns a span{ float:left; margin-top:3px;}

.nopadding { padding: 0 !important; margin: 0 !important;}



