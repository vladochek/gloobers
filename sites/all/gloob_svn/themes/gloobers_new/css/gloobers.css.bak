.map-search .sidebar .formsec .traveltype{ width:20%; float:left; position:relative; padding:0 3%;}
.map-search .sidebar .formsec .traveltype h2{ font-size:19px; font-weight:700; text-align:center; color:#424242; padding-bottom:5px;}
.map-search .sidebar .formsec .traveltype select{ border:1px solid #a9a9a9; height:30px; line-height:30px; width:100%; font-size:14px; color:#626262;}
.map-search .sidebar .formsec .ranktype{ width:30%; text-align:center; float:left; position:relative; padding:0 3%;}
.map-search .sidebar .formsec .ranktype h2{ font-size:19px; font-weight:700; text-align:center; color:#424242; padding-bottom:5px;}
.map-search .sidebar .formsec .ranktype .fa-star{ color:#797979; font-size:25px;}
.map-search .sidebar .formsec .ranktype .gold{color:#ffd479;}
.map-search .sidebar .formsec .spcloffer{ width:45%; text-align:center; float:left; position:relative; padding:0 10%;}
.map-search .sidebar .formsec .spcloffer h2{ font-size:19px; font-weight:700; text-align:center; color:#424242; padding-bottom:5px;}
.map-search .sidebar .formsec .spcloffer p{ font-size:16px; color:#424242;}
.map-search .sidebar .formsec .spcloffer .clickarrow{ float:left; width:100%; text-align:center;}
.map-search .sidebar .formsec .spcloffer .clickarrow a{ text-decoration:none; color:#535353;}

.map-search .sidebar .formsec .spcloffer .checksec { background:#ffffff; display:none; border:1px solid #494949; float:left; left:0; margin: auto; padding: 15px;  position: absolute; right: 0; top: 72px; width:70%;  z-index:1; text-align:left;z-index:99999;}
.map-search .sidebar .formsec .spcloffer .checksec .col{ float:left; width:100%; padding:5px 0;}
.map-search .sidebar .formsec .spcloffer .checksec .col span{ float:left; margin:4px 0 0 0;}
.map-search .sidebar .formsec .spcloffer .checksec .col label{ font-size:16px; color:#626262; padding-left:20px; float:left;}

.map-search .sidebar .formsec .pricerange{ width:20%; float:left; position:relative; padding:0 3%;}
.map-search .sidebar .formsec .pricerange h2{ font-size:19px; font-weight:700; text-align:center; color:#424242; padding-bottom:13px;}


.map-search .sidebar .formsec .pricerange .value { position: absolute;top: 30px; left: 50%; margin: 0 0 0 -20px; width: 40px; text-align: center; display: block; font-weight: normal;  font-family: Verdana,Arial,sans-serif;  font-size: 14px;  color: #333;}
.map-search .sidebar .formsec .pricerange .price-range-both.value { width: 100px; margin: 0 0 0 -50px; top: 26px;}
.map-search .sidebar .formsec .pricerange .price-range-both {  display: none; }
.map-search .sidebar .formsec .pricerange .value i {  font-style: normal;}

.map-search .sidebar .listsection{ float:left; width:100%; padding-bottom:0px;}
.map-search .sidebar .listsection .listing{ float:left; padding:0 0 30px 0;}
.map-search .sidebar .listsection .listing:nth-child(2n){ margin-left:2%;}
.map-search .sidebar .listsection .listing .block{ float:left; width:100%; position:relative; border:1px solid #e0e0e0;}
.map-search .sidebar .listsection .listing .block .img-height{ /*height:100%;*/ width:100%; float:left;}
.map-search .sidebar .listsection .listing .block .review{ background:rgba(0,0,0,0.5); width:60px; height:50px; position:absolute; right:7px; bottom:17px; border-radius:5px; border:2px solid #b7aca2;z-index:9999;}
.map-search .sidebar .listsection .listing .block .review p{ text-align:center; color:#fff; padding-top:5px; font-size:14px;}
.map-search .sidebar .listsection .listing .block .review p span{ font-size:16px; font-weight:700;}
.map-search .sidebar .listsection .listing .block .review .botarrow { bottom:-17px; left:11px; position:absolute; z-index:99;}
.map-search .sidebar .listsection .listing .block .clientpic{ width:70px; position:absolute; left:22px; bottom:-35px;z-index:9999;}
.map-search .sidebar .listsection .listing .block .clientpic img{ border-radius:100%; float:left; width:100%; border:1px solid #fff;}
.map-search .sidebar .listsection .listing .block .tagsec{ position:absolute; top:0px; right:10px; max-width:85px; width:100%;z-index:9999;}
.map-search .sidebar .listsection .listing .block .tagsec .tag{ width:29px; height:85px; padding:13px 3px; background:url(../images/icon5.png) 0 0 no-repeat; float:left;}
.map-search .sidebar .listsection .listing .block .tagsec .tag i{ float:left;}
.map-search .sidebar .listsection .listing .block .tagsec .tag p{color:#fff;text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5); font-size:9px; line-height:10px;}
.map-search .sidebar .listsection .listing .block .tagsec .price{ border:1px solid #b7aca2; background:rgba(0,0,0,0.5); line-height:30px; float:right; width:44px; height:30px; margin:5px 0 0 0;}
.map-search .sidebar .listsection .listing .block .tagsec .price p{ color:#fff; font-size:16px; font-weight:700; text-align:center; text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5);}
.map-search .sidebar .listsection .listing .block .listdetail{ background:rgba(0,0,0,0.5); max-width:165px; position:absolute; left:5px; top:5px; padding:5px;z-index:9999;}
.map-search .sidebar .listsection .listing .block .listdetail .click{ float:left; width:10px;}
.map-search .sidebar .listsection .listing .block .listdetail .click a{ display:block; float:left;}
.map-search .sidebar .listsection .listing .block .listdetail .click a span{ background:#fff; width:10px; height:4px; float:left; margin:1px 0;}
.map-search .sidebar .listsection .listing .block .listdetail .listview{ float:left; width:100%; padding:5px 0 0 0; display:none;}
.map-search .sidebar .listsection .listing .block .listdetail .listview ul{ list-style:none;}
.map-search .sidebar .listsection .listing .block .listdetail .listview ul li{ width:100%; float:left; line-height:24px;}
.map-search .sidebar .listsection .listing .block .listdetail .listview ul li i{ height:16px; width:16px; border:1px solid #fff; border-radius:100%; text-align:center; line-height:16px; margin-right:10px; font-size:10px;}
.map-search .sidebar .listsection .listing .block .listdetail .listview ul li a{ color:#fff; text-decoration:none; font-size:11px; text-shadow: 0px 2px 3px rgba(0, 0, 0, 0.5);}
.map-search .sidebar .listsection .listing .headingsec{ /*float:left;*/ width:100%; padding-left:100px; padding-bottom:35px; box-sizing:border-box;}
.map-search .sidebar .listsection .listing .headingsec h2{ font-size:17px; font-weight:700; color:#424242;}
.map-search .sidebar .listsection .listing .headingsec p{ font-size:14px; color:#626262;}
.map-search .sidebar .listsection .listing .headingsec p span{ color:#0092bf; float:right;}


.map-search .sidebar .paging-sec{ float:left; width:100%; text-align:center; padding-bottom:40px;}
.map-search .sidebar .paging-sec ul{ list-style:none;}
.map-search .sidebar .paging-sec ul li{ display:inline;}
.map-search .sidebar .paging-sec ul li a{ font-size:23px; color:#424242; text-decoration:none;}
.map-search .sidebar .paging-sec ul li a:hover{ color:#00afec; }
.map-search .sidebar .paging-sec ul li.select a{ color:#00afec; }
.map-search .maparea{ bottom:0; right:auto; position: fixed; left:0; top:156px; width:38.5%;}
.map-search .maparea iframe{ width:100%; height:100%; position:absolute;}
