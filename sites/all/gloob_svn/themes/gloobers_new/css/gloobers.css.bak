.form-item, .form-actions {margin-bottom: 0; margin-top: 0;}
/* Result Page CSS  */
.topbar{ padding:10px 0; background:#f0f0f0; float:left; width:100%; margin:94px 0 0 0;}
.headbar{ float:left; width:100%;}
.headbar input[type=text]{ float:left; padding:0 10px; width:100%; height:40px; color:#626262; line-height:40px; background:#fff; border:1px solid #a9a9a9; font-size:17px; max-width:234px; }
.headbar input[type=button]{ float:right; width:100%; height:40px; color:#333; line-height:40px; background:#f5c500; border:1px solid #a9a9a9; font-size:18px; max-width:146px; text-align:center; cursor:pointer;}
.headbar ul{ list-style:none; float:left; margin-left:55px;}
.headbar ul li{ float:left; padding:5px 0 0 0;}
.headbar ul li a{ text-decoration:none; color:#797979; font-size:21px; font-weight:700; padding:0 12px;}
.headbar ul li a:hover{ color:#212121;}
.headbar ul li.active a{ color:#212121;}
.inner-header{ background: #232323; /* Old browsers */
background: -moz-linear-gradient(top, #232323 0%, #333333 50%, #414141 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#232323), color-stop(50%,#333333), color-stop(100%,#414141)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #232323 0%,#333333 50%,#414141 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #232323 0%,#333333 50%,#414141 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #232323 0%,#333333 50%,#414141 100%); /* IE10+ */
background: linear-gradient(to bottom, #232323 0%,#333333 50%,#414141 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#232323', endColorstr='#414141',GradientType=0 ); /* IE6-9 */}
header .search{ float:left; margin:10px 0 0 30px; width:100%; max-width:360px;}
header .search input[type=text]{ float:left; padding:0 2%; width:100%; height:36px; color:#5e5e5e; line-height:36px; background:#fff; border:none; font-size:14px; }
header .admin-sec{ float:right; position:relative;}
header .admin-sec .arrow{ position:absolute; bottom:-14px; left:0; right:0; margin:auto; width:13px;}
header .admin-sec .admin-pic{ width:100%; float:left; border-radius:100%; border:1px solid #fff; max-width:55px;}
header .admin-sec .admin-pic img{ float:left; width:100%; height:40px;  border-radius: 100%;}
/****************************Results page****************************************/
.map-search{ float:left; width:100%;}
.map-search .sidebar {  bottom: 0;  right: 0; overflow-y: scroll; position: fixed; top:156px; width: 60%;}
.map-search .sidebar .formsec{ width:99.999%; float:left; border:1px solid #4b4b4b; padding:15px 0;  border-right:none; margin-bottom:15px;}
.map-search .sidebar .formsec .rowmap{ float:left; width:100%; border-top:1px solid #4b4b4b; padding:15px 0 0 0; margin-top:15px;}
.map-search .sidebar .formsec .exptype{ width:30%; float:left; position:relative; padding:0 3%; text-align:center;}
.map-search .sidebar .formsec .exptype h2{ font-size:19px; font-weight:700; text-align:center; color:#424242; padding-bottom:5px;}
.map-search .sidebar .formsec .exptype select{ border:1px solid #a9a9a9; height:30px; line-height:30px; /*width:100%;*/ font-size:14px; color:#626262;}
.map-search .sidebar .formsec .datetype{ width:45%; float:left; padding:0 10%;}
.map-search .sidebar .formsec .datetype h2{ font-size:19px; font-weight:700; text-align:center; color:#424242; padding-bottom:5px;}
.map-search .sidebar .formsec .datetype .inputctrl{ position:relative; padding-right:20px;  border:1px solid #a9a9a9;}
.map-search .sidebar .formsec .datetype .inputctrl input[type=text]{ height:30px; line-height:30px; width:90%; border:none; font-size:14px; color:#626262; padding:0 5%;}
